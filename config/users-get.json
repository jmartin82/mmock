{  
    "description": "Gets a persisted entity by matching the id from the url using regex",
    "request":{  
        "method":"GET",
        "path":"/users/:userid.json"
    },
    "response":{  
        "statusCode":200,
        "headers":{  
            "Content-Type":[  
                "application/json"
            ]
        },
        "body":"{{transform.merge(request.path.userid,request.body)}}"
    }
}