{
	"description": "Checks for a given cookie in the request to match the configuration",
	"request": {
		"method": "GET",
		"path": "/hello*",
		"headers": {},
		"cookies": {
			"visit" : "true"
		}
	},
	"response": {
		"statusCode": 200,
		"body": "Hello AGAIN! my friend!"
	},
	"control":{
		"priority":1
	}
}