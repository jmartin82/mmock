{  
   "request":{  
      "method":"GET",
      "path":"/id-users/*"
   },
   "persist":{  
      "collection":"users/user-{{request.url.(?P<value>\\d+)}}.json"
   },
   "response":{  
      "statusCode":200,
      "headers":{  
         "Content-Type":[  
            "application/json"
         ]
      },
      "body":"{{persist.collection.content}}"
   }
}